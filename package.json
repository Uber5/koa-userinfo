{
  "name": "koa-userinfo",
  "version": "0.0.3",
  "description": "openid style userinfo as middleware, for koa",
  "main": "dist/index.js",
  "scripts": {
    "build": "babel src --out-dir dist",
    "test": "flow check && jest src",
    "test:watch": "npm test -- --watch",
    "coverage": "jest --coverage src",
    "clean": "rimraf dist",
    "prepublish": "npm run clean && npm test && npm run build",
    "postpublish": "npm run clean"
  },
  "keywords": [
    "koa",
    "openid",
    "oauth2",
    "userinfo"
  ],
  "author": "Chris Oloff <chris@uber5.com> (http://about.uber5.com/)",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git@github.com:Uber5/koa-userinfo.git"
  },
  "devDependencies": {
    "babel-cli": "6.26.0",
    "babel-jest": "20.0.3",
    "babel-preset-env": "1.6.0",
    "babel-preset-flow": "6.23.0",
    "babel-register": "6.26.0",
    "flow-bin": "0.54.0",
    "flow-typed": "2.1.5",
    "jest": "20.0.4",
    "koa": "2.3.0",
    "rimraf": "2.6.1"
  },
  "dependencies": {
    "debug": "3.0.1"
  }
}
